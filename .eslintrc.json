{
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-return-await": "off",
    },
    "ignorePatterns": "/client/"
}
